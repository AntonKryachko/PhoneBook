GET     /                    controllers.HomeController.index(error: String ?= null)

GET    /addNewUser          controllers.HomeController.addNewUser

GET     /getAllUsers         controllers.HomeController.getAllUsers

GET    /getUser             controllers.HomeController.getUser(id: String)

GET    /updateUser          controllers.HomeController.updateUser

GET    /removeUser/:id        controllers.HomeController.removeUser(id: String)

GET    /fullSearch/:name      controllers.HomeController.fullSearch(name: String)

GET    /fullSearch/      controllers.HomeController.fullSearch(name: String = "")

GET     /assets/*file        controllers.Assets.versioned(path="/public", file: Asset)

